define                   = 
integrator               = sd
dt                       = 0.002
nsteps                   = 500000
nstxout                  = 5000000
nstvout                  = 5000000
nstlog                   = 5000
nstenergy                = 5000
nstxtcout                = 10000

nstcalcenergy            = 5
nstlist                  = 20
nstcomm                  = 5
comm_mode                = Linear
comm-grps                = Water_and_ions_CLM Protein_Lipid SPRP
ns_type                  = grid
cutoff-scheme		     = Verlet
rlist                    = 0.9
vdwtype			         = pme
vdw-modifier 		     = potential-shift
coulombtype              = pme
DispCorr	    	     = no
rcoulomb                 = 0.9
rvdw                     = 0.9
fourierspacing		     = 0.10
pme_order		         = 4
ewald_rtol		         = 1e-6
ewald-rtol-lj            = 0.001

tcoupl                   = v-rescale ;nose-hoover	
